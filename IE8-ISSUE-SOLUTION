	<script type="text/javascript">
		var reportExtra = "";
	  function checkUncheckAll(theElement) {
			var theForm = theElement.form, z = 0, parent = "", allStatus = true;
			for(z = 0; z < theForm.length; z++) {
				if(theForm[z].type == "checkbox" && !theForm[z].disabled) {
					if (theElement.className == "allbox") theForm[z].checked = theElement.checked;
					else {
						if (theForm[z].checked == false && theForm[z].className != "allbox") allStatus = theForm[z].checked;
						if (theForm[z].name == theElement.name) theForm[z].checked = theElement.checked;
					}
				}
			}
			if (theElement.className != "allbox" && !theForm.allbox.disabled) theForm.allbox.checked = allStatus;
		}
		function checkCheckboxes(theElement) {
			var status = "deny";
			var checkboxNames = [];
			var numberChecked = 0;
			if (theElement.value.search(/approve/i) != -1) status = "approve";
			var theForm = theElement.form, z = 0;
			for(z = 0; z < theForm.length; z++) {
				if(theForm[z].type == "checkbox" && theForm[z].checked == true) {
				  if ($.inArray(theForm[z].name, checkboxNames) < 0 && theForm[z].name != "allbox") {
				    checkboxNames.push(theForm[z].name);
				    numberChecked += 1;
				  }
				  //numberChecked += 1;
				}
			}
			//alert (numberChecked);
			if (numberChecked > 0 && status == "approve") {
			  alert("You selected " + numberChecked + " award(s) to approve, but you only have 0 available.");
			  return false;
			}
			
			if(numberChecked > 0) {
			  return confirm("Are you sure you wish to " + status.toUpperCase() + " the selected award(s)?");
			}
			
			alert("You must select at least one award to " + status.toUpperCase() + "!");
			//theForm.select1.focus();
			return false;
		}
		function constrain(str,n) {
	    if(str.length > n) {
	      var s = str.substr(0, n);
	      var words = s.split(' ');
	      words[words.length-1] = '';
	      str = words.join(' ') + '&hellip;'
	    }
	    return str;
	  }
	  function truncateMiddle(str, chars) {
	    if (str.length > chars) {
	      // Favor the beginning of the string:
	      var half = Math.floor(chars / 2);
	      var endPos = str.length - half;
	      half += chars % 2;
	      str = str.substring(0, half) + '...' + str.substring(endPos);
	    }
	    return str;
	  }
	
