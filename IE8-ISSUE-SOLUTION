		}*/
		
		YAHOO.example.DynamicData = function() {
				var awardViewUrl = "awardview.cfm?temp=ZdPNhWDWlPMsiUdlrSPT94x6pGqpGNj4Qo";
				var currUrl = "/rmi/reports.cfm?temp=ZdPNhWDWlPMsiUdlrSPT94x6pGqpGNj4Qo";
				var awardUrl = "/rmi/award.cfm?temp=ZdPNhWDWlPMsiUdlrSPT94x6pGqpGNj4Qo";
				this.customDateLink = function(elCell, oRecord, oColumn, oData) {
					//height = 650;
					//width = 550;
					//tp = (screen.height/2) - height/2;
					//left = (screen.width/2) - width/2;
					//if (window.screenTop) tp = tp + window.screenTop - 150; else tp = tp + window.screenY;
					//if (window.screenLeft) left = left + window.screenLeft; else left = left + window.screenX;
					var url = awardViewUrl+"&page=issue&type=" + oRecord.getData("delivery") + "&cart=" + oRecord.getData("id");
					var html = "<a href='"+url+"' target='_blank'>";
					//html += " onClick='window.open('"+url+"','awardDetails','top=" + tp + ",left=" + left + ",height=" + height + ",";
					//html += "width=" + width + ",scrollbars=yes'); return false;'>";
					html += oRecord.getData("date") + "</a>";
					elCell.innerHTML = html;
				};
				YAHOO.widget.DataTable.Formatter.customDateLink = this.customDateLink;
 
				this.customFromLink = function(elCell, oRecord, oColumn, oData) {
					var url = currUrl+"&page=issue&start_month=02";
					url += "&start_day=1&start_year=2014&end_month=02";
					url += "&end_day=28&end_year=2014";
					url += "&from=" + oRecord.getData("from") + " (" + oRecord.getData("fromID") + ")";
					elCell.innerHTML = "<a href='"+url+"'>" + oRecord.getData("from") + "</a>";
				};
				YAHOO.widget.DataTable.Formatter.customFromLink = this.customFromLink;
 
				this.customToLink = function(elCell, oRecord, oColumn, oData) {
					var url = currUrl + "&page=issue&start_month=02&start_day=1";
					url += "&start_year=2014&end_month=02&end_day=28";
					url += "&end_year=2014&to=" + oRecord.getData("to") + " (" + oRecord.getData("toID") + ")";
					elCell.innerHTML = "<a href='"+url+"'>" + oRecord.getData("to") + "</a>";
				};
				YAHOO.widget.DataTable.Formatter.customToLink = this.customToLink;
 
				this.customUpgradeLink = function(elCell, oRecord, oColumn, oData) {
					if (oRecord.getData("upgrade") != "No") {
						var url = awardUrl + "&allEmployees=" + oRecord.getData("toID");
						url += "&core101=" + oRecord.getData("core1") + "&upgrade=" + oRecord.getData("id");
						url += "&awardtype01=" + oRecord.getData("awardType");
						elCell.innerHTML = "<a href='"+url+"'>" + oRecord.getData("upgrade") + "</a>";
					}
				};
				YAHOO.widget.DataTable.Formatter.customUpgradeLink = this.customUpgradeLink;
 
				this.customDocsLink = function(elCell, oRecord, oColumn, oData) {
					if (oRecord.getData("docs") != "") {
						var doclist = oRecord.getData("docs");
						var valueArray = doclist.split(",");
						var doclinks = "";
						var docimage = "";
						if (valueArray.length > 1) {
							for(var i=0; i<valueArray.length; i++) {
								var docext = valueArray[i].split(".");
								if (docext[docext.length - 1].indexOf("doc") != -1) docimage = "doc.gif";
								else if (docext[docext.length - 1].indexOf("xls") != -1) docimage = "xls.gif";
								else if (docext[docext.length - 1].indexOf("pdf") != -1) docimage = "pdf.gif";
								else if (docext[docext.length - 1].indexOf("ppt") != -1) docimage = "ppt.gif";
								else if (docext[docext.length - 1].indexOf("txt") != -1) docimage = "txt.gif";
								else var docimage = "file.gif";
								doclinks = doclinks + "<img src=\"/rmi/images/caregiver/" + docimage + "\" border=\"0\" />" + "<a href=\"http://corp1.corporaterewards.com/rmi/sites/caregiver/uploads/" + oRecord.getData("id") + "/" + valueArray[i] + "\" target=\"blank\">" + truncateMiddle(valueArray[i], 30) + "</a><br />";
							}
						} else {
							if (doclist.indexOf("doc") != -1) {
								docimage = "doc.gif";
							} else if (doclist.indexOf("xls") != -1) {
								docimage = "xls.gif";
							} else if (doclist.indexOf("pdf") != -1) {
								docimage = "pdf.gif";
							} else if (doclist.indexOf("ppt") != -1) {
								docimage = "ppt.gif";
							} else {
								var docimage = "";
							}
							doclinks = "<img src=\"/rmi/images/caregiver/" + docimage + "\" border=\"0\" />" + "<a href=\"http://corp1.corporaterewards.com/rmi/sites/caregiver/uploads/" + oRecord.getData("id") + "/" + doclist + "\" target=\"blank\">" + truncateMiddle(doclist, 30) + "</a>";
						}
						elCell.innerHTML = doclinks;
					}
				};
				YAHOO.widget.DataTable.Formatter.customDocsLink = this.customDocsLink;
 
				// Column definitions
				var myColumnDefs = [
					
					{key:"date", label:"Date", sortable:true, formatter:"customDateLink"},
					{key:"from", label:"Sender", sortable:true, formatter:"customFromLink"},
					{key:"to", label:"Recipient", sortable:true, formatter:"customToLink"},
					{key:"awardType", label:"Award Tier", sortable:true},
					{key:"amount", label:"Amount", sortable:true},
					{key:"status", label:"Status", sortable:true}
				];
				
				
				// DataSource instance
				var myDataSource = new YAHOO.util.DataSource("/rmi/sites/caregiver/_reports.cfm?page=issue&temp=ZdPNhWDWlPMsiUdlrSPT94x6pGqpGNj4Qo&results=50&start_date=02/01/2014&end_date=02/28/2014&");
				myDataSource.responseType = YAHOO.util.DataSource.TYPE_JSON;
				myDataSource.responseSchema = {
					resultsList: "records",
					fields: [
						{key:"id", parser:"number"},
						
						{key:"date"},
						{key:"from"},
						{key:"fromID"},
						{key:"to"},
						{key:"toID"},
						{key:"awardType"},
						{key:"core1"},
						{key:"amount"},
						
						{key:"delivery"},
						{key:"status"}
					],
					metaFields: {
						totalRecords: "totalRecords" // Access to value in the server response
					}
				};
 
				// DataTable configuration
				var myConfigs = {
					initialRequest: "sort=date&dir=desc&startIndex=0&cache=0", // Initial request for first page of data
					dynamicData: true, // Enables dynamic server-driven data
					sortedBy : {key:"date", dir:YAHOO.widget.DataTable.CLASS_DESC}, // Sets UI initial sort arrow
					paginator: new YAHOO.widget.Paginator({ rowsPerPage:50 }) // Enables pagination
				};
				
				
				// DataTable instance
				var myDataTable = new YAHOO.widget.DataTable("dynamicdata", myColumnDefs, myDataSource, myConfigs);
				// Update totalRecords on the fly with value from server
				myDataTable.handleDataReturnPayload = function(oRequest, oResponse, oPayload) {
					oPayload.totalRecords = oResponse.meta.totalRecords;
					if (oResponse.meta.totalRecords > 0)
					{
						$("#reportRowText").html(oResponse.meta.totalRecords + " row(s) ");
					}else{
						$("#downloadCSVLink").hide();
					}
					return oPayload;
				}
 
				return {
					ds: myDataSource,
					dt: myDataTable
				};
 
			}();
	</script>
 
		<div class="clear"></div>
		<div class="container footer">
			<div class="content">
				<p>
					&copy; 
					2014. Cleveland Clinic, all rights reserved.
				</p>
			</div>
		</div>
	</div>
</body>
</html>

